--GAMEPASS --STR
local ReplicatedStorage = game:GetService("ReplicatedStorage") 
local function task()
local Event = game:GetService("ReplicatedStorage").Remotes.Dumbbell
Event:FireServer()
local Event = game:GetService("ReplicatedStorage").Remotes.Dumbbell
Event:FireServer()
local Event = game:GetService("ReplicatedStorage").Remotes.Dumbbell
Event:FireServer()
end
while true do
    for i = 1, 1 do
        task()
        wait()  
    end
    wait(0.5)
end
